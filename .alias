alias v='vim'
alias ..='cd ..'
alias rm='yes | rm'
alias tree='tree -C'
alias va='vim `find . | fzf --color=fg+:11 --height 90% --reverse --select-1 --exit-0 --multi --preview "head -n 300 {}"`'
alias pre='find . | fzf --color=fg+:11 --height 90% --reverse --select-1 --exit-0 --multi --preview "head -n 300 {}"'

alias gs='git status'
alias ga='git add -A'
alias gc="git commit -m ${1}"
alias gp="git push"
alias gb='git branch -avv'


function f() {
  find ./ -name "*${1}*"
}
